{
  "extends": "gts/tslint.json",
  "rules": {
    "no-default-export": false,
    // This rule is here for now because of https://github.com/palantir/tslint-react/issues/120
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "max-line-length": [
      true,
      100
    ]
  }
}
