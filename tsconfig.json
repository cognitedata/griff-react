{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "declaration": false,
    "rootDir": ".",
    "outDir": "build",
    "skipLibCheck": true,
    "jsx": "react",
    "allowJs": true,
    "target": "es5",
    "module": "es2015",
    "lib": ["es2015", "dom"],
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": [
    "src/**/*",
    "stories/**/*"
  ]
}
